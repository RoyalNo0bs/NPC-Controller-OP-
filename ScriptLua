Title_25.Position = UDim2.new(0.0153122684, 0, 0.08271911, 0)
Title_25.Size = UDim2.new(0.617193282, 0, 0.617193639, 0)
Title_25.Font = Enum.Font.Gotham
Title_25.Text = "NPC Panel [OP🔥]"
Title_25.TextColor3 = Color3.fromRGB(255, 255, 255)
Title_25.TextScaled = true
Title_25.TextSize = 14.000
Title_25.TextWrapped = true
Title_25.TextXAlignment = Enum.TextXAlignment.Left

local plr = game.Players.LocalPlayer
local MainFolderSelected = workspace:FindFirstChild(plr.Name.."SpawnedInToys")
local CurrentTarget = plr.Character
local WalkToPoint = true
local MoveLook = false

function MoveTo(NPC,Target)

	local function isLookingAt(head, targetPart, maxAngle)
		local headLookVector = head.CFrame.LookVector
		local directionToTarget = (targetPart.Position - head.Position).Unit
		local angle = math.acos(headLookVector:Dot(directionToTarget)) -- Angle in radians
		return angle <= math.rad(maxAngle)
	end


	WalkSpeed.Active = false
	Input.TextEditable = true
	Input_3.TextEditable = true
	Input_2.TextEditable = true
	if NPC and NPC:FindFirstChild("Humanoid") and Target and Target.Torso then
		if not isLookingAt(Target.Torso, NPC.HumanoidRootPart,65) and MoveLook == true or MoveLook == false then
			local Humanoid = NPC:FindFirstChild("Humanoid")
			Humanoid:MoveTo(Vector3.new(Target.Torso.Position.X,Target.Torso.Position.Y,Target.Torso.Position.Z))
			Humanoid.WalkSpeed = WalkSpeed.Input.Text
			Humanoid.JumpPower = JumpPower.Input.Text
		elseif MoveLook == true and isLookingAt(Target.Torso, NPC.HumanoidRootPart,65) then
			local Humanoid = NPC:FindFirstChild("Humanoid")
			Humanoid:MoveTo(NPC.Torso.Position)
		end
	else
		local Humanoid = NPC:FindFirstChild("Humanoid")
		Humanoid:MoveTo(NPC.Torso.Position)
	end
end

function AutoAssignFolder()
	if Plot1.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		MainFolderSelected = workspace:FindFirstChild("PlotItems"):FindFirstChild("Plot1")
	elseif Plot2.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		MainFolderSelected = workspace:FindFirstChild("PlotItems"):FindFirstChild("Plot2")
	elseif Plot3.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		MainFolderSelected = workspace:FindFirstChild("PlotItems"):FindFirstChild("Plot3")
	elseif Plot4.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		MainFolderSelected = workspace:FindFirstChild("PlotItems"):FindFirstChild("Plot4")
	elseif Plot5.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		MainFolderSelected = workspace:FindFirstChild("PlotItems"):FindFirstChild("Plot5")
	elseif SpawnedInToys.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		MainFolderSelected = workspace:FindFirstChild(plr.Name.."SpawnedInToys")
	end
end

function AddPlayerToList(Player)
	local Username = Player.Name
	local ImageSize = Enum.ThumbnailSize.Size60x60 -- Thumbnail Size
	local ImageType = Enum.ThumbnailType.AvatarBust -- Thumbnail Type
	local ImageId = game.Players:GetUserThumbnailAsync(Player.UserId, ImageType, ImageSize) -- Gets Image from UserId

	local Clone = PlayerList.Contents.TemplateIcon:Clone()

	Clone.Parent = PlayerList.Contents
	Clone.Name = Username
	Clone.Title.Text = Username
	Clone.Image = ImageId
	Clone.Visible = true
	Clone.MouseButton1Click:Connect(function()
		for _, Button in ipairs(PlayerList.Contents:GetChildren()) do
			if Button:IsA("ImageButton") then
				Button.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
				Button.BackgroundTransparency = 1
			end
		end
		CurrentTarget = game.Players:FindFirstChild(Clone.Name).Character or game.Players:FindFirstChild(Clone.Name).CharacterAdded
		Clone.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
		Clone.BackgroundTransparency = 0.5
	end)
end

for _,Player in ipairs(game.Players:GetPlayers()) do
	AddPlayerToList(Player)
end

game.Players.PlayerAdded:Connect(function(plr)
	AddPlayerToList(plr)
end)

game.Players.PlayerRemoving:Connect(function(plr)
	PlayerList.Contents:FindFirstChild(plr.Name):Destroy()
end)
function CreateProfile()

	local ImageSize = Enum.ThumbnailSize.Size420x420 -- Thumbnail Size
	local ImageType = Enum.ThumbnailType.AvatarBust -- Thumbnail Type
	local ImageId = game.Players:GetUserThumbnailAsync(plr.UserId, ImageType, ImageSize)
	PlayerIcon.Image = ImageId

	Username.Text = plr.Name
	UserID.Text = plr.UserId
	DisplayName.Text = plr.DisplayName
end

CreateProfile()

for _, TextButton in ipairs(MainFolder:GetChildren()) do
	if TextButton:IsA("TextButton") then
		TextButton.MouseButton1Click:Connect(function()
			for _, TextButton2 in ipairs(MainFolder:GetChildren()) do
				if TextButton2:IsA("TextButton") then
					TextButton2.BackgroundColor3 = Color3.fromRGB(34, 33, 39)
					TextButton2:FindFirstChild("Title").TextColor3 = Color3.new(1, 1, 1)
				end
			end

			TextButton.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
			TextButton:FindFirstChild("Title").TextColor3 = Color3.new(0.133333, 0.129412, 0.152941)
			AutoAssignFolder()
		end)
	end
end

ESP.MouseButton1Click:Connect(function()
	if ESP.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		ESP.BackgroundColor3 = Color3.fromRGB(34, 33, 39)
		ESP.Title.TextColor3 = Color3.new(1, 1, 1)
		for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
			if NPC:IsA("Model") and NPC:FindFirstChild("Humanoid") then
				repeat
					if NPC:FindFirstChild("ESPHighlight") then
						NPC:FindFirstChild("ESPHighlight"):Destroy()
					end
				until NPC:FindFirstChild("ESPHighlight") == nil
			end
		end
	else
		ESP.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
		ESP.Title.TextColor3 = Color3.new(0.133333, 0.129412, 0.152941)
		for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
			if NPC:IsA("Model") and NPC:FindFirstChild("Humanoid") then
				local ESPHighlight = Instance.new("Highlight")
				ESPHighlight.Parent = NPC
				ESPHighlight.Name = "ESPHighlight"
				ESPHighlight.Enabled = true
			end
		end
	end
end)

ViewPlayer.MouseButton1Click:Connect(function()
	if ViewPlayer.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		ViewPlayer.BackgroundColor3 = Color3.fromRGB(34, 33, 39)
		ViewPlayer.Title.TextColor3 = Color3.new(1, 1, 1)
		workspace.CurrentCamera.CameraSubject = plr.Character.Humanoid
	else
		ViewPlayer.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
		ViewPlayer.Title.TextColor3 = Color3.new(0.133333, 0.129412, 0.152941)
		workspace.CurrentCamera.CameraSubject = CurrentTarget.Humanoid
	end
end)

