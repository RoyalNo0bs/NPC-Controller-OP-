	if MoveWhenLooked.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		MoveWhenLooked.BackgroundColor3 = Color3.fromRGB(34, 33, 39)
		MoveWhenLooked.Title.TextColor3 = Color3.new(1, 1, 1)
		MoveLook = false
	else
		MoveWhenLooked.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
		MoveWhenLooked.Title.TextColor3 = Color3.new(0.133333, 0.129412, 0.152941)
		MoveLook = true
	end
end)

PlotBarriers.MouseButton1Click:Connect(function()
	if PlotBarriers.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		PlotBarriers.BackgroundColor3 = Color3.fromRGB(34, 33, 39)
		PlotBarriers.Title.TextColor3 = Color3.new(1, 1, 1)
		for _, Boarder in ipairs(workspace.Plots:GetDescendants()) do
			if Boarder:IsA("BasePart") and Boarder.Name == "PlotBarrier" then
				Boarder.CanCollide = true
			end
		end
	else
		PlotBarriers.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
		PlotBarriers.Title.TextColor3 = Color3.new(0.133333, 0.129412, 0.152941)
		for _, Boarder in ipairs(workspace.Plots:GetDescendants()) do
			if Boarder:IsA("BasePart") and Boarder.Name == "PlotBarrier" then
				Boarder.CanCollide = false
			end
		end
	end
end)

WalkOnWater.MouseButton1Click:Connect(function()
	if WalkOnWater.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		WalkOnWater.BackgroundColor3 = Color3.fromRGB(34, 33, 39)
		WalkOnWater.Title.TextColor3 = Color3.new(1, 1, 1)
		for _, ocean in ipairs(workspace.Map.AlwaysHereTweenedObjects.Ocean.Object.ObjectModel:GetChildren()) do
			if ocean:IsA("BasePart") and ocean.Name == "Ocean" then
				ocean.CanCollide = false
			end
		end
	else
		WalkOnWater.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
		WalkOnWater.Title.TextColor3 = Color3.new(0.133333, 0.129412, 0.152941)
		for _, ocean in ipairs(workspace.Map.AlwaysHereTweenedObjects.Ocean.Object.ObjectModel:GetChildren()) do
			if ocean:IsA("BasePart") and ocean.Name == "Ocean" then
				ocean.CanCollide = true
			end
		end
	end
end)

WalkToPlayer.MouseButton1Click:Connect(function()
	WalkSpeed.Active = true
	if WalkToPlayer.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		WalkToPoint = false
		WalkToPlayer.BackgroundColor3 = Color3.fromRGB(34, 33, 39)
		WalkToPlayer.Title.TextColor3 = Color3.new(1, 1, 1)
	else
		WalkToPoint = true
		WalkToPlayer.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
		WalkToPlayer.Title.TextColor3 = Color3.new(0.133333, 0.129412, 0.152941)
	end
end)

ThirdPerson.MouseButton1Click:Connect(function()
	if ThirdPerson.BackgroundColor3 == Color3.fromRGB(53, 255, 87) then
		ThirdPerson.BackgroundColor3 = Color3.fromRGB(34, 33, 39)
		ThirdPerson.Title.TextColor3 = Color3.new(1, 1, 1)
		game.Players.LocalPlayer.CameraMode = Enum.CameraMode.LockFirstPerson
		game.Players.LocalPlayer.CameraMaxZoomDistance = 0.5
	else
		ThirdPerson.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
		ThirdPerson.Title.TextColor3 = Color3.new(0.133333, 0.129412, 0.152941)
		game.Players.LocalPlayer.CameraMode = Enum.CameraMode.Classic
		game.Players.LocalPlayer.CameraMaxZoomDistance = 100000
	end
end)

SetNetworkOwner.MouseButton1Click:Connect(function()
	for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
		if NPC:IsA("Model") and NPC:FindFirstChild("HumanoidRootPart") and CurrentTarget and CurrentTarget.Torso then

			game.ReplicatedStorage.GrabEvents.SetNetworkOwner:FireServer(NPC.HumanoidRootPart, NPC.HumanoidRootPart.CFrame)
			game.ReplicatedStorage.GrabEvents.DestroyGrabLine:FireServer(NPC.HumanoidRootPart)
		end
	end
end)


Jump.MouseButton1Click:Connect(function()
	for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
		if NPC:IsA("Model") and NPC:FindFirstChild("HumanoidRootPart") and CurrentTarget and CurrentTarget.Torso then

			local rootPart = NPC:WaitForChild("HumanoidRootPart")

			local force = Instance.new("VectorForce")
			force.Attachment0 = rootPart:FindFirstChildOfClass("Attachment") or Instance.new("Attachment", rootPart)
			force.Force = Vector3.new(0, tonumber(JumpPower.Input.Text)*100, 0) 
			force.RelativeTo = Enum.ActuatorRelativeTo.World
			force.Parent = rootPart

		end
	end
	wait(0.2)
	for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
		if NPC:IsA("Model") and NPC:FindFirstChild("HumanoidRootPart") and CurrentTarget and CurrentTarget.Torso then
			local rootPart = NPC:WaitForChild("HumanoidRootPart")
			local force = rootPart:FindFirstChildOfClass("VectorForce")
			force:Destroy()
		end
	end
end)

Teleport.MouseButton1Click:Connect(function()
	for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
		if NPC:IsA("Model") and NPC:FindFirstChild("HumanoidRootPart") and CurrentTarget and CurrentTarget.Torso then
			NPC.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0,0,0)
			NPC.HumanoidRootPart.CFrame = CFrame.new(CurrentTarget.Torso.CFrame.X,CurrentTarget.Torso.CFrame.Y,CurrentTarget.Torso.CFrame.Z)
		end
	end
end)

Freeze.MouseButton1Click:Connect(function()
	if Freeze.BackgroundColor3 == Color3.fromRGB(34, 33, 39) then
		for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
			if NPC and NPC:IsA("Model") and NPC:FindFirstChild("Humanoid") and NPC:FindFirstChild("HumanoidRootPart") then
				local bodyVelocity = Instance.new("BodyVelocity")
				bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
				bodyVelocity.Velocity = Vector3.zero
				bodyVelocity.P = 10000
				bodyVelocity.Parent = NPC.HumanoidRootPart
				bodyVelocity.Name = "FreezeVelocity"
				Freeze.BackgroundColor3 = Color3.fromRGB(53, 255, 87)
				Freeze.Title.TextColor3 = Color3.new(0.133333, 0.129412, 0.152941)

			end
		end
	else
		for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
			if NPC and NPC:IsA("Model") and NPC:FindFirstChild("Humanoid") and NPC:FindFirstChild("HumanoidRootPart") then
				if NPC.HumanoidRootPart:FindFirstChild("FreezeVelocity") then
					NPC.HumanoidRootPart:FindFirstChild("FreezeVelocity"):Destroy()
				end
			end
		end
		Freeze.BackgroundColor3 = Color3.fromRGB(34, 33, 39)
		Freeze.Title.TextColor3 = Color3.new(1, 1, 1)
	end
end)

local Loop = game:GetService("RunService").Stepped:Connect(function()
	if WalkToPoint == true then
		for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
			if NPC:IsA("Model") and NPC:FindFirstChild("Humanoid") then
				MoveTo(NPC,CurrentTarget)
			end
		end
	end
	game:GetService("UserInputService").MouseEnabled = true
	game:GetService("UserInputService").MouseIconEnabled = false
	game:GetService("UserInputService").MouseBehavior = Enum.MouseBehavior.Default
end)

plr:GetMouse().KeyDown:Connect(function(Key)
	if Key == "m" then
		NPCPanel.Enabled = not NPCPanel.Enabled
	end
end)

NPCPanel.Destroying:Connect(function()
	WalkToPoint = false
	game.Players.LocalPlayer.CameraMode = Enum.CameraMode.LockFirstPerson
	Loop:Disconnect()
	for _, NPC in ipairs(MainFolderSelected:GetChildren()) do
		if NPC:IsA("Model") and NPC:FindFirstChild("Humanoid") then
			if NPC.HumanoidRootPart:FindFirstChild("FreezeVelocity") then
				NPC.HumanoidRootPart:FindFirstChild("FreezeVelocity"):Destroy()
			end
		end
	end
end)

local UserInputService = game:GetService("UserInputService")
local runService = (game:GetService("RunService"));


local gui = Panel

local dragging
local dragInput
local dragStart
local startPos

function Lerp(a, b, m)
	return a + (b - a) * m
end;

local lastMousePos
local lastGoalPos
local DRAG_SPEED = (8); -- // The speed of the UI darg.
function Update(dt)
	if not (startPos) then return end;
	if not (dragging) and (lastGoalPos) then
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
		return 
	end;

	local delta = (lastMousePos - UserInputService:GetMouseLocation())
	local xGoal = (startPos.X.Offset - delta.X);
	local yGoal = (startPos.Y.Offset - delta.Y);
	lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
	gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
end;

TopBar.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position
		lastMousePos = UserInputService:GetMouseLocation()

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

TopBar.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

runService.Heartbeat:Connect(Update)
